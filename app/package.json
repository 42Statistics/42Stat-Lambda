{
  "name": "app",
  "version": "0.0.1",
  "description": "",
  "main": "./dist/index.js",
  "type": "module",
  "scripts": {
    "build": "rm -rf ./dist && tsc -p tsconfig.build.json && cp ../env/.env ./",
    "start": "node ./dist/index.js",
    "compile": "pnpm build && rm -rf deploy && mkdir -p deploy && pnpm ncc build dist/index.js -o deploy && cp ../env/.env ./",
    "prepare": "pnpm compile && zip -rjD deploy/index.zip deploy/* .env",
    "start:dev": "cp ../.env ./ && pnpm start",
    "lint:peek": "eslint \"{src,apps,libs,test}/**/*.ts\" --cache",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "imports": {
    "#lambda/*": "./dist/*"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@types/node": "^18.18.8",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.5.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "type-fest": "^3.9.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.433.0",
    "camelcase-keys": "^8.0.2",
    "dotenv": "^16.0.3",
    "la-seine": "^0.0.7",
    "mongodb": "^5.3.0",
    "redis": "^4.6.6",
    "zod": "^3.21.4"
  }
}
