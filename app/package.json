{
  "name": "app",
  "version": "0.0.1",
  "description": "",
  "main": "./dist/index.js",
  "type": "module",
  "scripts": {
    "build": "rm -rf ./dist && tsc -p tsconfig.build.json && cp ../env/.env ./",
    "start": "node ./dist/index.js",
    "dev": "pnpm build && pnpm start",
    "compile": "rm -rf ./dist && pnpm ncc build src/index.ts -o dist && cp ../env/.env ./",
    "prepare": "pnpm compile && rm -rf deploy && mkdir -p deploy && zip -rjD deploy/index.zip dist/* .env",
    "start:dev": "cp ../.env ./ && pnpm start",
    "lint:peek": "eslint \"{src,apps,libs,test}/**/*.ts\" --cache",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@types/node": "^18.16.3",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "type-fest": "^3.9.0",
    "typescript": "5.0.4"
  },
  "dependencies": {
    "camelcase-keys": "^8.0.2",
    "dotenv": "^16.0.3",
    "la-seine": "^0.0.6",
    "mongodb": "^5.3.0",
    "redis": "^4.6.6",
    "zod": "^3.21.4"
  }
}
